/* pages/material/process.wxss */
.container {
  min-height: 100%;
}
.weui-cell {
  width: 100%;
  background: #fff;
}
.weui-media-box {
  display: flex;
  justify-content: space-between;
}
.weui-media-box-rt {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: space-between;
  font-size: 13px;
}
.weui-media-box-rt button {
  padding: 0 0.8em;
  margin-right: 0;
}
.weui-media-box__desc__time {
  color: #999;
  font-size: 12px;
  margin-top: 20rpx;
}